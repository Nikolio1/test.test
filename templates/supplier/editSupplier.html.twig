{% extends 'base.html.twig' %}


{% block body %}



    {{ form_start(form) }}
    <div class="form-control">
        {{ form_label(form.name,'Supplier name') }}
        <div class="form-error">
            {{ form_errors(form.name) }}
        </div>
        {{ form_widget(form.name) }}
        <small>{{ form_help(form.name) }}</small>

    </div>

    <div class="form-control">
     {{ form_label(form.contactName,'Supplier contactName') }}
        <div class="form-error">
            {{ form_errors(form.contactName) }}
        </div>
        {{ form_widget(form.contactName) }}
        <small>{{ form_help(form.contactName) }}</small>
    </div>

    <div class="form-control">
        {{ form_label(form.phone) }}
        <div class="form-error">
            {{ form_errors(form.phone) }}
        </div>
        {{ form_widget(form.phone) }}
        <small>{{ form_help(form.phone) }}</small>

    </div>
    <div class="form-control">
        {{ form_label(form.email) }}
        <div class="form-error">
            {{ form_errors(form.email) }}
        </div>
        {{ form_widget(form.email) }}
        <small>{{ form_help(form.email) }}</small>

    </div>

    <button type="submit" >Save</button>

    {{ form_end(form) }}
    <a href="{{ path('suppliers') }}">Назад</a>

{% endblock body %}